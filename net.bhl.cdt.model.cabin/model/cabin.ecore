    <eStructuralFeatures xsi:type="ecore:EReference" name="lavatories" lowerBound="1"
        upperBound="-1" eType="#//Lavatory" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="galleys" lowerBound="1"
        upperBound="-1" eType="#//Galley" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="stairs" upperBound="-1"
        eType="#//Stairway" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="curtain" upperBound="-1"
        eType="#//Curtain" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="stowage" upperBound="-1"
        eType="#//Stowage" containment="true"/>
