  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="StairwayDirection">
    <eLiterals name="up" value="1"/>
    <eLiterals name="down" value="-1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="SeatsPerRow">
    <eLiterals name="Economy" value="6"/>
    <eLiterals name="Business" value="4"/>
    <eLiterals name="First" value="2" literal=""/>
